			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Response status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"",
							"pm.test(\"Response has the required fields - id, name, address, latitude, longitude, and distance\", function () {",
							"    const responseData = pm.response.json();",
							"    ",
							"    pm.expect(responseData).to.be.an('array').that.is.not.empty;",
							"    ",
							"    responseData.forEach(function(school) {",
							"        pm.expect(school).to.have.property('id');",
							"        pm.expect(school).to.have.property('name');",
							"        pm.expect(school).to.have.property('address');",
							"        pm.expect(school).to.have.property('latitude');",
							"        pm.expect(school).to.have.property('longitude');",
							"        pm.expect(school).to.have.property('distance');",
							"    });",
							"});",
							"",
							"",
							"pm.test(\"Latitude and longitude are in valid format\", function () {",
							"  const responseData = pm.response.json();",
							"  ",
							"  pm.expect(responseData).to.be.an('array');",
							"  responseData.forEach(function(school) {",
							"    pm.expect(school.latitude).to.be.a('number');",
							"    pm.expect(school.longitude).to.be.a('number');",
							"  });",
							"});",
							"",
							"",
							"pm.test(\"Distance is a non-negative integer\", function () {",
							"    const responseData = pm.response.json();",
							"    ",
							"    pm.expect(responseData).to.be.an('array');",
							"    responseData.forEach(function(school) {",
							"        pm.expect(school.distance).to.be.a('number');",
							"        pm.expect(school.distance).to.be.at.least(0);",
							"    });",
							"});",
							"",
							"",
							"pm.test(\"Content-Type header is application/json\", function () {",
							"    pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\");",
							"});"
						],
						"type": "text/javascript"
					}
				}
